<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Exercises.LogExercise.LogExercises">

    <include
        android:id = "@+id/toolbar"
        layout="@layout/toolbar"
        />

    <TextView
        android:id="@+id/idCurrentPlan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="102dp"
        android:text="Current Plan"
        android:textAlignment="center"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/idLogExerciseFragmentListPH"
        android:name="com.example.olympia.Exercises.LogExercise.FragmentLogExerciseList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/idCurrentPlan"
        android:layout_marginTop="20dp"
        />




</RelativeLayout>

