<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WorkoutRoutines.WorkoutRoutines.TricepWorkout"
    tools:ignore="SpeakableTextPresentCheck">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <include
            android:id = "@+id/toolbar"
            layout="@layout/toolbar"
            />

        <CheckedTextView
            android:id="@+id/checkedTextView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Ez Curl Bar or Dumbell Lying Tricep Extension" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/lyingtricepexten" />
        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/lyingdumbeltri" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets)" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:text="Tricep Cable Pushdown" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            app:srcCompat="@drawable/tricepspushdown" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets) " />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Rope/Straight Bar Pushdown" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/ropepulldown" />


        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets) " />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:text="Dumbell Overhead Tricep Extension" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/overheadtri" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets)" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Close Grip Bench Press" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/closegripbecnh" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 90 sec in between sets)" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Kickback" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/kickback" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets)" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Reverse Grip Cable Extension" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/reversegriptri" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets)" />

        <Space
            android:layout_width="match_parent"
            android:layout_height="100dp"></Space>

        <CheckedTextView
            android:id="@+id/checkedTextView12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:checkMark="@android:drawable/checkbox_off_background"
            android:checked="false"
            android:text="Single Arm Cable Tricep Extension Reverse/Supinated Grip" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/singlearmcabletri" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            app:srcCompat="@drawable/singlearmsupinatedtri" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="4 sets, 8-10 reps (rest 60 sec in between sets)" />

    </LinearLayout>

</ScrollView>

